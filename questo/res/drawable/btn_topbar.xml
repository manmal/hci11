<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<gradient android:startColor="#2c0701" android:endColor="#000000"
				android:type="linear" android:angle="270" android:dither="true" />
		</shape>
	</item>
	<item android:left="1dp">
		<shape android:shape="rectangle">
			<gradient android:startColor="#722e26" android:endColor="#52110c"
				android:type="linear" android:angle="270" android:dither="true" />
		</shape>
	</item>
	<item android:left="2dp" android:top="1dp">
		<shape android:shape="rectangle">
			<gradient android:startColor="@color/winered_light" android:endColor="@color/winered_gone_dark"
				android:type="linear" android:angle="270" android:dither="true" />
		</shape>
	</item>
</layer-list>