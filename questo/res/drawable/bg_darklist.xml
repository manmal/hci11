<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<solid android:color="#3c1411" />
		</shape>
	</item>
	<item android:top="1dp">
		<shape android:shape="rectangle" >
			<solid android:color="@color/winered_verydark" />
		</shape>
	</item>
	<item android:top="1dp">
		<shape android:shape="rectangle" >
			<gradient android:startColor="#00000000"
				android:endColor="#40000000" android:type="linear"
				android:angle="270" android:dither="true" />
		</shape>
	</item>
	<item>
		<bitmap android:src="@drawable/q_watermark" android:gravity="center" />
	</item>
</layer-list>