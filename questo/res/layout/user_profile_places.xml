<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent">
	<ListView android:layout_height="fill_parent"
		android:layout_width="match_parent" android:layout_alignParentLeft="true"
		android:id="@+id/ProfilePlaceList" android:background="@color/winered_verydark" />
	<TextView android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/empty_places_text"
		android:text="No places visited yet!" style="@style/WhiteShadowedFont"
		android:padding="10dp" android:gravity="center_horizontal"
		android:textSize="14sp" android:layout_above="@+id/bottombar"
		android:background="@color/winered_verydark" />
</RelativeLayout>
